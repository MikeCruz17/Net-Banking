@model IEnumerable<Entidades.Reporte_Usuarios>

@{
    ViewBag.Title = "ReporteUsuario";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";

}

<br />
<h2>Lista de Usuarios</h2>
<hr />

<div class="col-md-offset-2 col-md-10">
    <br />
    <a class="btn btn-success" href="/Home/RegistroCliente">Añadir</a>
    <a class="btn btn-primary" href="/Home/Administracion">Volver a Inicio</a>
    <br />
    <br />
</div>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Cedula)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Apellido)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fecha_Nacimiento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Sexo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Telefono)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Correo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Contraseña)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Rol)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Estatus)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {

            if (item.Estatus == "Habilitado")
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Cedula)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Nombre)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Apellido)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Fecha_Nacimiento)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Sexo)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Telefono)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Correo)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Contraseña)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Rol)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Estatus)
                    </td>
                    <td>
                        <a class="btn btn-danger" href="/Home/EditarEstado/@item.Cedula">Deshabilitar</a>
                        <a class="btn btn-warning" href="/Home/EditarUsuario/@item.Cedula">Editar</a>
                    </td>
                </tr>
            }
        }

    </table>
